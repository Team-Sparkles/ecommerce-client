<tr>
   <th>Remove</th>
   <th>Qty</th>
   <th>Item</th>
   <th>Price</th>
 </tr>

{{#each items as |item|}}
    <tr data-id="{{item._id}}" data-price="{{item.price}}" class="table-primary">
      <td><button class="remove-item-button" data-id="{{item._id}}" type="button">Remove</button></td>
      <td>1</td>
      <td>{{item.name}}</td>
      <td>{{item.dollarAmount}}</td>
    </tr>
{{/each}}
